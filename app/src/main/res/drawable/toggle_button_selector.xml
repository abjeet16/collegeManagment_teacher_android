<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- State when the toggle is selected (Present) -->
    <item android:state_checked="true" android:drawable="@color/presentColor" />

    <!-- State when the toggle is not selected (Absent) -->
    <item android:state_checked="false" android:drawable="@color/absentColor" />
</selector>
